<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="528.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coach.LineChartController">
   <children>
      <LineChart fx:id="lineChart" layoutY="60.0" prefHeight="408.0" prefWidth="614.0" title="LINE CHART">
         <xAxis>
            <CategoryAxis label="My X Label" side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis label="My Y Label" side="LEFT" />
         </yAxis>
      </LineChart>
      <Button layoutX="278.0" layoutY="484.0" mnemonicParsing="false" onMouseClicked="#btnAction" text="Load Chart" />
      <ComboBox fx:id="combo" layoutX="36.0" layoutY="27.0" prefWidth="150.0" />
      <Button fx:id="delete" layoutX="241.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#deleteAction" text="Button" />
   </children>
</AnchorPane>
